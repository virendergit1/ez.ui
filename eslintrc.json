{
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "angular": true,
    "_": true,
    "$": true,
    "requirejs": true,
    "document": true,
    "define": true,
    "window": true,
    "host": true,
    "describe": false,
    "it": false,
    "expect": false,
    "beforeEach": false,
    "afterEach": false,
    "spyOn": false,
    "inject": false,
    "url": false,
    "jasmine": false,
    "Globalize": false,
    "FormData": true,
    "XMLHttpRequest": true,
    "sinon": false,
    "Blob": false,
    "URL": false,
    "tpsApiModule": false,
    "viewSpecModule": false,
    "vsrViewSpecModule": false,
    "dashboardViewSpecModule": false,
    "ArrayBuffer": false
  },
  "plugins": [ "angular" ],
  "rules": {
    "angular/angularelement": 0,
    "angular/controller-as": 2,
    "angular/controller-as-route": 0,
    "angular/controller-as-vm": 2,
    "angular/controller-name": 0,
    "angular/deferred": 0,
    "angular/definedundefined": 0,
    "angular/di": 0,
    "angular/di-order": 0,
    "angular/directive-name": 2,
    "angular/component-limit": 0,
    "angular/document-service": 0,
    "angular/empty-controller": 0,
    "angular/file-name": 0,
    "angular/filter-name": 2,
    "angular/foreach": 0,
    "angular/function-type": 0,
    "angular/interval-service": 2,
    "angular/json-functions": 0,
    "angular/log": 0,
    "angular/module-getter": 0,
    "angular/module-name": 2,
    "angular/module-setter": 0,
    "angular/no-angular-mock": 2,
    "angular/no-controller": 0,
    "angular/no-cookiestore": 0,
    "angular/no-jquery-angularelement": 2,
    "angular/no-private-call": 0,
    "angular/no-service-method": 0,
    "angular/no-services": 2,
    "angular/on-watch": 0,
    "angular/rest-service": 2,
    "angular/service-name": 2,
    "angular/timeout-service": 2,
    "angular/typecheck-array": 0,
    "angular/typecheck-date": 0,
    "angular/typecheck-function": 0,
    "angular/typecheck-number": 0,
    "angular/typecheck-object": 0,
    "angular/typecheck-string": 0,
    "angular/watchers-execution": [0, "$digest"],
    "angular/window-service": 0,
    "block-scoped-var": 1,
    "camelcase": 0,
    "comma-style":0,
    "curly": 2,
    "dot-notation": 0,
    "eqeqeq":2,
    "no-eq-null":2,
    "global-strict": 0,
    "guard-for-in": 2,
    "new-cap": 0,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": 2,
    "no-debugger": 2,
    "no-empty": 0,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [
      2,      
      {
        "functions": false
      }
    ],
    "no-with": 2,
    "no-console": 0,
    "quotes": [
      0,
      "single"
    ],
    "semi": 2,
    "no-extra-semi": 2,
    "strict": 0,
    "valid-typeof": 2,
    "wrap-iife": 0
  }
}